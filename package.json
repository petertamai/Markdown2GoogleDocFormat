{
    "name": "markdown-to-gdoc-converter",
    "version": "1.0.0",
    "description": "API to convert markdown to Google Docs format using OAuth authentication",
    "main": "src/server.js",
    "scripts": {
      "start": "node src/server.js",
      "dev": "nodemon src/server.js",
      "pm2:start": "pm2 start ecosystem.config.js",
      "pm2:stop": "pm2 stop ecosystem.config.js",
      "pm2:restart": "pm2 restart ecosystem.config.js",
      "pm2:status": "pm2 status",
      "pm2:logs": "pm2 logs",
      "pm2:monitor": "pm2 monit",
      "test": "jest --detectOpenHandles",
      "lint": "eslint .",
      "format": "prettier --write \"**/*.{js,json,md}\""
    },
    "author": "Piotr Tamulewicz <pt@petertam.pro>",
    "license": "MIT",
    "dependencies": {
      "cors": "^2.8.5",
      "dotenv": "^16.3.1",
      "express": "^4.18.2",
      "express-rate-limit": "^7.1.4",
      "express-validator": "^7.0.1",
      "google-auth-library": "^9.4.1",
      "googleapis": "^129.0.0",
      "helmet": "^7.1.0",
      "marked": "^11.2.0",
      "morgan": "^1.10.0",
      "winston": "^3.11.0"
    },
    "devDependencies": {
      "eslint": "^8.55.0",
      "eslint-config-prettier": "^9.1.0",
      "eslint-plugin-jest": "^27.6.0",
      "eslint-plugin-node": "^11.1.0",
      "jest": "^29.7.0",
      "nodemon": "^3.0.2",
      "prettier": "^3.1.0",
      "supertest": "^6.3.3"
    },
    "engines": {
      "node": ">=16.0.0"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/yourusername/markdown-to-gdoc-converter.git"
    },
    "keywords": [
      "markdown",
      "google-docs",
      "api",
      "converter",
      "n8n",
      "oauth",
      "pm2"
    ]
  }